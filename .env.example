# Trading 212 API Credentials
# Copy this file to .env and fill in your values
#
# Get your API key and secret from:
# Trading 212 app > Settings > API (Beta) > Generate API Key

TRADING212_API_KEY=your_api_key_here
TRADING212_API_SECRET=your_api_secret_here

# Environment: 'live' for real account, 'demo' for paper trading
TRADING212_ENVIRONMENT=live
